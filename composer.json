{
    "name": "joyching/laravel-i18n-tool",
    "description": "A Laravel package providing artisan commands to import and export language files from and to CSV.",
    "authors": [
        {
            "name": "Joy Lin",
            "email": "kanaheijoy@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/console": "~5.5",
        "illuminate/support": "~5.5",
        "illuminate/translation": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Joyching\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joyching\\I18n\\Tests\\": "tests/"
        },
        "files": [
            "tests/Fake/helpers.php"
        ]
    }
}
